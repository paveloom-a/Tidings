<?xml version="1.0" encoding="UTF-8"?>
<!-- Pavel Sobolev @year@ <paveloom@riseup.net> -->
<component type="desktop-application">
  <id>@app-id@</id>
  <metadata_license>CC0</metadata_license>
  <project_license>GPL-3.0</project_license>
  <name>@app-name-capitalized@</name>
  <summary>Finally, an RSS reader without a built-in browser</summary>
  <description>
    <p>Yet another RSS reader, but this time it's just two lists and an optional description panel.</p>
  </description>
  <screenshots>
    <screenshot type="default">
      <image>Missing link</image>
      <caption>Main window</caption>
    </screenshot>
  </screenshots>
  <url type="homepage">https://github.com/paveloom-a/Tidings</url>
  <url type="bugtracker">https://github.com/paveloom-a/Tidings/issues</url>
  <content_rating type="oars-1.0" />
  <releases>
    <release version="@version@" date="@date@" />
  </releases>
  <kudos>
     <!--
       GNOME Software kudos:
       https://gitlab.gnome.org/GNOME/gnome-software/blob/master/doc/kudos.md
     -->
     <kudo>ModernToolkit</kudo>
     <kudo>HiDpiIcon</kudo>
  </kudos>
  <developer_name>Pavel Sobolev</developer_name>
  <update_contact>paveloom@riseup.net</update_contact>
  <translation type="gettext">@domainname@</translation>
  <launchable type="desktop-id">@app-id@.desktop</launchable>
</component>
